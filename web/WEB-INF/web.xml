<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>image</servlet-name>
        <servlet-class>view.ViewImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>oddoneout</servlet-name>
        <servlet-class>controller.OddOneOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>errorPage</servlet-name>
        <jsp-file>/WEB-INF/jspf/error.jspf</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>gamePage</servlet-name>
        <jsp-file>/WEB-INF/jspf/oddoneout.jspf</jsp-file>
    </servlet>
    <servlet><servlet-name>scorePage</servlet-name><jsp-file>/WEB-INF/jspf/score.jspf</jsp-file></servlet><servlet-mapping>
        <servlet-name>image</servlet-name>
        <url-pattern>/image</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>oddoneout</servlet-name>
        <url-pattern>/oddoneout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list></web-app>
